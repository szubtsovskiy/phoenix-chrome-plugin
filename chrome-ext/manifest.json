{
  "name": "Phoenix Channels Testing",
  "version": "1.0",
  "description": "A plug-in to test interaction via Phoenix channels",
  "manifest_version": 2,
  "permissions": ["tabs"],
  "icons": { "16": "phoenix-framework.png", "48": "phoenix-framework.png", "128": "phoenix-framework.png" },
  "browser_action": {
    "default_icon": "phoenix-framework.png"
  },
  "background": {
    "scripts": ["background.js"]
  }

}
