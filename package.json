{
  "name": "phoenix-chrome-plugin",
  "description": "A plug-in for Chrome browser to test interaction via Phoenix channels",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Sergey Zubtsovskiy",
  "repository": {
    "type": "git",
    "url": "https://github.com/szubtsovskiy/phoenix-chrome-plugin"
  },
